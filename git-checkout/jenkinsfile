pipeline{
    agent any 
    stages{
        stage('checkout stage'){
            steps{
                git branch: 'jenkins',  url: 'https://github.com/ManojKRISHNAPPA/career-devops-micro.git'
            }
        }
        
        stage('Build'){
            steps{
                echo "Building the webooks.sh file"
                sh './webhook.sh'
            }
        }
    }
}